                            <option value="mercedes">xyz</option>
                            <option value="audi">lmn</option>
                        </select>
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6">
                    <div class="dropdown">
                        <select>
                            <option value="" selected disabled hidden>Reviews</option>
                            <option value="saab">abc</option>
                            <option value="mercedes">xyz</option>
                            <option value="audi">lmn</option>
                        </select>
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6">
                    <div class="dropdown">
                        <select>
                            <option value="" selected disabled hidden>Opening Hours</option>
                            <option value="saab">abc</option>
                            <option value="mercedes">xyz</option>
                            <option value="audi">lmn</option>
                        </select>
